<html>
<head>
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.0/css/jquery.dataTables.css" />
    <script src="https://code.jquery.com/jquery-1.11.1.min.js" type="text/javascript"></script>
    <script src="https://cdn.datatables.net/1.10.0/js/jquery.dataTables.js" type="text/javascript"></script>
    <script>
        $(function() {
            $('#datatable').dataTable({{ datatable_options(dataTable) }});
        });
    </script>
</head>
<body>
    <table id="datatable">
        {{ datatable_body(dataTable) }}
    </table>
</body>
</html>